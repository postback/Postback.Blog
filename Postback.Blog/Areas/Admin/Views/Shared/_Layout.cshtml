@using Postback.Blog
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>Manage that Content - @ViewBag.Title</title>
    @Rzr.Css("backend")

    @RenderSection("head", required: false)
</head>
<body>
    <div class="container">
        <header>
            <h1>Admin</h1>
            @Html.Partial("_Nav")
        </header>
        <section id="main">
            @RenderBody()
        </section>
        <footer>
        </footer>
    </div>

    @Rzr.Js("backendscripts")
    @RenderSection("Scripts", required: false)
</body>
</html>
